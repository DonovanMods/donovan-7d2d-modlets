  <append xpath="items/item[starts-with(@name, 'meleeToolAxe')]/property[@name='Tags']/@value">,perkDeepCuts</append>
  <!-- Included from donovan-bettercement -->
  <set xpath="/items/item[@name='unit_stone']/property[@name='CraftingIngredientTime']/@value">0.01</set>
  <set xpath="/items/item[@name='resourceClayLump']/property[@name='CraftingIngredientTime']/@value">0.01</set>
  <append xpath="/items/item[@name='resourceCrushedSand']">
    <property name="CraftingIngredientTime" value="0.01"/>
  </append>
