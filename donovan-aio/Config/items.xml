  <set xpath="/items/item[@name='ammoJunkTurretRegular']/property[@name='Stacknumber']/@value">5000</set>
  <set xpath="/items/item[@name='ammoGasCan']/property[@name='Stacknumber']/@value">30000</set>
  <set xpath="/items/item[@name='casinoCoin']/property[@name='Stacknumber']/@value">25000</set>
  <set xpath="/items/item[@name='foodRawMeat']/property[@name='Stacknumber']/@value">1000</set>
  <set xpath="/items/item[@name='foodRottingFlesh']/property[@name='Stacknumber']/@value">1000</set>
  <set xpath="/items/item[@name='foodEgg']/property[@name='Stacknumber']/@value">500</set>
  <set xpath="/items/item[@name='medicalAloeCream']/property[@name='Stacknumber']/@value">500</set>
  <set xpath="/items/item[@name='medicalBloodBag']/property[@name='Stacknumber']/@value">500</set>
  <set xpath="/items/item[@name='partsMaster']/property[@name='Stacknumber']/@value">10</set>
  <set xpath="/items/item[@name='schematicMaster']/property[@name='Stacknumber']/@value">10</set>
  <set xpath="/items/item[@name='smallEngine']/property[@name='Stacknumber']/@value">12</set>
  <set xpath="/items/item[@name='resourceLockPick']/property[@name='Stacknumber']/@value">500</set>
  <!-- Included from donovan-craftables -->
  <!-- Acid -->
  <append xpath="/items/item[@name='resourceAcid']/property[@name='Group']/@value">,Chemicals</append>
  <append xpath="/items/item[@name='resourceAcid']">
    <property name="UnlockedBy" value="resourceAcidSchematic"/>
  </append>
  <append xpath="/items">
    <item name="resourceAcidSchematic">
      <property name="Extends" value="schematicNoQualityRecipeMaster"/>
      <property name="CreativeMode" value="Player"/>
      <property name="CustomIcon" value="resourceAcid"/>
      <property name="Unlocks" value="resourceAcid"/>
      <effect_group tiered="false">
        <triggered_effect trigger="onSelfPrimaryActionEnd" action="ModifyCVar" cvar="resourceAcid" operation="set" value="1"/>
        <triggered_effect trigger="onSelfPrimaryActionEnd" action="GiveExp" exp="100"/>
      </effect_group>
    </item>
  </append>
  <!-- Sewing Kit -->
  <append xpath="/items/item[@name='resourceSewingKit']">
    <property name="UnlockedBy" value="resourceSewingKitSchematic"/>
  </append>
  <append xpath="/items">
    <item name="resourceSewingKitSchematic">
      <property name="Extends" value="schematicNoQualityRecipeMaster"/>
      <property name="CreativeMode" value="Player"/>
      <property name="CustomIcon" value="resourceSewingKit"/>
      <property name="Unlocks" value="resourceSewingKit"/>
      <effect_group tiered="false">
        <triggered_effect trigger="onSelfPrimaryActionEnd" action="ModifyCVar" cvar="resourceSewingKit" operation="set" value="1"/>
        <triggered_effect trigger="onSelfPrimaryActionEnd" action="GiveExp" exp="50"/>
