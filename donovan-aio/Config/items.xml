  <set xpath="items/item[@name='meleeWpnBladeT3Machete']/effect_group[@name='meleeWpnBladeT3Machete']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">30</set>
  <append xpath="items/item[@name='meleeWpnBladeT3Machete']/effect_group[@name='Power Attack']">
    <passive_effect name="EntityDamage" operation="base_add" value="15" tags="secondary"/>
  </append>
  <!-- Spears -->
  <set xpath="items/item[@name='meleeWpnSpearT0StoneSpear']/effect_group[@name='meleeWpnSpearT0StoneSpear']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">15</set>
  <set xpath="items/item[@name='meleeWpnSpearT1IronSpear']/effect_group[@name='meleeWpnSpearT1IronSpear']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">20</set>
